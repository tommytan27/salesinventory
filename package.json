{
  "name": "salesinventory",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aspnet/signalr-client": "^1.0.0-alpha2-final",
    "bootstrap": "^3.3.7",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.2.0",
    "react-md": "^1.2.11",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.17",
    "react-select": "^1.2.1",
    "redux": "^3.7.2",
    "webfontloader": "^1.6.28"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./enzyme.config.js"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && npm run build-css -- --watch --recursive",
    "start": "concurrently \"npm run watch-css\" \"react-scripts start\" \"dotnet run --project signalRConnection\\signalRConnection.csproj\" --kill-others",
    "build": "npm run build-css && react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-jest": "^22.0.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.0.4",
    "node-sass-chokidar": "0.0.3",
    "react-test-renderer": "^16.2.0"
  }
}
